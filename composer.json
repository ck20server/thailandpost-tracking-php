{
    "name": "farzai/thailand-post",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PA",
            "role": "DevOps"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\ThaiPost\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\Tests\\":  "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
